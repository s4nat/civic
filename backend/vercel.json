{
    "version": 2,
    "builds": [
      {
        "src": "/api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/donation(.*)",
          "dest": "api/index.js"
        },
        {
          "src": "/company(.*)",
          "dest":"api/index.js"
        },
        {
          "src": "/drive(.*)",
          "dest": "api/index.js"
        },
        {
          "src": "/project(.*)",
          "dest": "api/index.js"
        },
        {
          "src": "/user(.*)",
          "dest": "api/index.js"
        },
        {
          "src": "/(.*)",
          "dest": "api/index.js"
        }
      ]
  }
